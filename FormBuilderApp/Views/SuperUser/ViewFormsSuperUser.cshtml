@model IEnumerable<FormBuilderApp.Models.Form>

@{
    ViewBag.Title = "View Forms";
}

<h2>Forms Pending Approval</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
            </td>
        </tr>
    }
